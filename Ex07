package main

import "fmt"

func main() {
	array := [5]int{1, 2, 3, 4, 5}
	var slice []int
	var numero int
	fmt.Print("Informe um n√∫mero: ")
	fmt.Scan(&numero)

	for i := 0; i < len(array); i++ {
		if numero == i {
			continue
		} else {
			break
			slice = append(slice, array[i], numero)
		}
		fmt.Print(slice)
	}
}
